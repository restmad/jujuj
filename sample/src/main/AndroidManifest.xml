<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shinado.netframe.sample">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:name="sample.MyApplication"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
        <meta-data android:name="AA_DB_NAME" android:value="jujuj.db" />
        <meta-data android:name="AA_DB_VERSION" android:value="2" />
        
        <activity android:name="sample.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="jujuj.post.PostActivity" />
        <activity android:name="jujuj.proxy.ProxyActivity"/>
        <activity android:name="jujuj.validate.ValidateActivity"/>
        <activity android:name="jujuj.downloadable.Demo2Activity" />
        <activity android:name="jujuj.multiple.Demo3Activity" />
        <activity android:name="jujuj.Loadable.Demo4Activity" />
        <activity android:name="jujuj.multibean.Demo5Activity" />
        <activity android:name="jujuj.multibean.UserActivity" />
        <activity android:name="jujuj.dependent.Demo6Activity" />
        <activity android:name="jujuj.listable.Demo7Activity" />
        <activity android:name="jujuj.action.Demo8Activity" />
    </application>

</manifest>
