<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shinado.netframe.sample">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:name="sample.MyApplication"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
        <meta-data android:name="AA_DB_NAME" android:value="jujuj.db" />
        <meta-data android:name="AA_DB_VERSION" android:value="2" />
        
        <activity android:name="sample.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="jujuj.demo1.Demo1Activity" />
        <activity android:name="jujuj.demo2.Demo2Activity" />
        <activity android:name="jujuj.demo3.Demo3Activity" />
        <activity android:name="jujuj.demo4.Demo4Activity" />
        <activity android:name="jujuj.demo5.Demo5Activity" />
        <activity android:name="jujuj.demo5.UserActivity" />
        <activity android:name="jujuj.demo6.Demo6Activity" />
        <activity android:name="jujuj.demo7.Demo7Activity" />
        <activity android:name="jujuj.demo8.Demo8Activity" />
    </application>

</manifest>
